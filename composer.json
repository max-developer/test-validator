{
  "name": "mwdevel/validator",
  "type": "library",
  "authors": [
    {
      "name": "Max Rautkin",
      "email": "mrautkin@gmail.com"
    }
  ],
  "description": "php-package",
  "license": "MIT",
  "scripts": {
    "test": "phpunit tests"
  },
  "autoload": {
    "psr-4": {
